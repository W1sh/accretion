<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="600.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ResultsController">
    <TableView fx:id="resultsTable" prefHeight="600.0" prefWidth="400.0">
      <columns>
          <TableColumn fx:id="colPoster" editable="false" maxWidth="100.0" prefWidth="100.0" resizable="false" text="Poster" />
          <TableColumn fx:id="colInfo" editable="false" maxWidth="300.0" minWidth="0.0" prefWidth="300.0" resizable="false" text="Info" />
      </columns>
    </TableView>
</VBox>
